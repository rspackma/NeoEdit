<Menu Name="self" x:Class="NeoEdit.Processes.ProcessesMenu"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:guiCommon="clr-namespace:NeoEdit.GUI.Common;assembly=NeoEdit.GUI"
	xmlns:processes="clr-namespace:NeoEdit.Processes">

	<MenuItem Header="_View">
		<processes:ProcessesMenuItem Header="_Refresh" CommandEnum="View_Refresh"/>
		<processes:ProcessesMenuItem Header="_Handles" CommandEnum="View_Handles"/>
		<processes:ProcessesMenuItem Header="_Memory" CommandEnum="View_Memory"/>
		<processes:ProcessesMenuItem Header="M_odules" CommandEnum="View_Modules"/>
	</MenuItem>
	<MenuItem Header="_Process">
		<processes:ProcessesMenuItem Header="_Suspend" CommandEnum="Process_Suspend"/>
		<processes:ProcessesMenuItem Header="_Resume" CommandEnum="Process_Resume"/>
		<processes:ProcessesMenuItem Header="_Kill" CommandEnum="Process_Kill"/>
	</MenuItem>
	<guiCommon:WindowMenu/>
	<guiCommon:HelpMenu/>

</Menu>