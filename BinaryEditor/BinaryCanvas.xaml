<UserControl x:Class="NeoEdit.BinaryEditor.BinaryCanvas"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	xmlns:guiCommon="clr-namespace:NeoEdit.GUI.Common;assembly=GUI"
	mc:Ignorable="d" ClipToBounds="True"
	d:DesignWidth="300" d:DesignHeight="300"
	Background="White" Focusable="True" FocusVisualStyle="{x:Null}">

	<guiCommon:RenderCanvas x:Name="canvas" Grid.Row="0" Grid.Column="0" Background="Transparent" Margin="10,5" ClipToBounds="True"/>

</UserControl>
