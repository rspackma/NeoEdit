<controls:NEWindow x:Name="self" x:Class="NeoEdit.UI.Dialogs.Execute_Network_WCF_InterceptCalls_Dialog"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:controls="clr-namespace:NeoEdit.UI.Controls"
	Title="WCF Intercept Calls" SizeToContent="WidthAndHeight">

	<Grid Margin="10">
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition MinHeight="10"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>

		<TextBlock Grid.Row="0" Grid.Column="0" Foreground="White" Text="Waiting for WCF calls..."/>

		<UniformGrid Grid.Row="2" HorizontalAlignment="Right" Rows="1">
			<Button IsDefault="True" IsCancel="True" Content="Done" Padding="10,1"/>
		</UniformGrid>
	</Grid>

</controls:NEWindow>
