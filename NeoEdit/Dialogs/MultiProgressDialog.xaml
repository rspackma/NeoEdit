<controls:NEWindow x:Name="self" x:Class="NeoEdit.Program.Dialogs.MultiProgressDialog"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:controls="clr-namespace:NeoEdit.Program.Controls"
	Title="Progress" Width="800" Height="600">

	<Grid Margin="10">
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>

		<Grid Grid.Row="0" Grid.Column="0" Name="progressGrid"/>

		<Button Grid.Row="1" Grid.Column="0" Content="Cancel" Padding="10,1" HorizontalAlignment="Center" Click="OnCancel"/>
	</Grid>

</controls:NEWindow>
