<Window x:Name="self" x:Class="NeoEdit.GUI.Clipboard.ClipboardWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Clipboard" Width="500" Height="370" Visibility="Visible" FocusManager.FocusedElement="{Binding ElementName=items}">

	<ListBox Name="items" ItemsSource="{Binding Records, ElementName=self}" Height="Auto" />

</Window>
