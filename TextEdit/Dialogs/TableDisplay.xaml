<UserControl Name="self" x:Class="NeoEdit.TextEdit.Dialogs.TableDisplay" x:ClassModifier="internal"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Focusable="True" IsTabStop="True">

	<ScrollViewer Name="scroller" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Hidden" Focusable="False">
		<Grid Name="tableGrid" Background="LightGray" MouseDown="GridMouseDown"/>
	</ScrollViewer>

</UserControl>
